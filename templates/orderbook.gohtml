                                      <option value="PIRATE">PIRATE (ARRR)</option>
                                    </select>
                                    <div class="input-group-append">
                                      <label class="input-group-text" for="inputGroupSelect02">Rel</label>
                                    </div>
                                  </div>
                                </td>
                                <td>KMD</td>
                              </tr>
                            </table>

                          </div>
                        </form>
                    </div> <!-- coin info body end -->
                </div> <!-- coin info card end -->
